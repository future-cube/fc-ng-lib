{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "future-cube": {
      "projectType": "library",
      "root": "projects/future-cube",
      "sourceRoot": "projects/future-cube/src",
      "prefix": "fc",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/future-cube/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/future-cube/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/future-cube/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/future-cube/src/test.ts",
            "tsConfig": "projects/future-cube/tsconfig.spec.json",
            "karmaConfig": "projects/future-cube/karma.conf.js"
          }
        }
      }
    },
    "theme": {
      "projectType": "library",
      "root": "projects/theme",
      "sourceRoot": "projects/theme/src",
      "prefix": "fc",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/theme/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/theme/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/theme/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/theme/src/test.ts",
            "tsConfig": "projects/theme/tsconfig.spec.json",
            "karmaConfig": "projects/theme/karma.conf.js"
          }
        }
      }
    },
    "library-tester": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "less"
        }
      },
      "root": "projects/library-tester",
      "sourceRoot": "projects/library-tester/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/library-tester",
            "index": "projects/library-tester/src/index.html",
            "main": "projects/library-tester/src/main.ts",
            "polyfills": "projects/library-tester/src/polyfills.ts",
            "tsConfig": "projects/library-tester/tsconfig.app.json",
            "inlineStyleLanguage": "less",
            "assets": [
              "projects/library-tester/src/favicon.ico",
              "projects/library-tester/src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
                "output": "/assets/"
              }
            ],
            "styles": [
              "projects/library-tester/src/styles.less"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/library-tester/src/environments/environment.ts",
                  "with": "projects/library-tester/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "library-tester:build:production"
            },
            "development": {
              "browserTarget": "library-tester:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "library-tester:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/library-tester/src/test.ts",
            "polyfills": "projects/library-tester/src/polyfills.ts",
            "tsConfig": "projects/library-tester/tsconfig.spec.json",
            "karmaConfig": "projects/library-tester/karma.conf.js",
            "inlineStyleLanguage": "less",
            "assets": [
              "projects/library-tester/src/favicon.ico",
              "projects/library-tester/src/assets"
            ],
            "styles": [
              "projects/library-tester/src/styles.less"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
